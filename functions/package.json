  "main": "lib/index.js",
  "dependencies": {
    "@google-cloud/pubsub": "^4.3.3",
    "@google-cloud/vertexai": "^1.10.0",
    "dayjs": "^1.11.9",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^6.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/jest": "^29.5.14",
    "firebase-functions-test": "^3.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^4.9.0"
  },
  "private": true
