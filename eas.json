{
  "cli": {
    "version": ">= 16.26.0"
  },
  "build": {
    "development": {
      "distribution": "store",
      "env": {
        "EAS_USE_CACHE": "1"
      },
      "cache": {
        "paths": [
          "./ios/Pods",
          "./ios/Podfile.lock"
        ]
      },
      "android": {
        "gradleCommand": ":app:bundleDevRelease"
      },
      "ios": {
        "scheme": "CanaryTrainLCD",
        "buildConfiguration": "Release"
      }
    },
    "preview": {
      "distribution": "store",
      "env": {
        "EAS_USE_CACHE": "1"
      },
      "cache": {
        "paths": [
          "./ios/Pods",
          "./ios/Podfile.lock"
        ]
      }
    },
    "production": {
      "distribution": "store",
      "env": {
        "EAS_USE_CACHE": "1"
      },
      "cache": {
        "paths": [
          "./ios/Pods",
          "./ios/Podfile.lock"
        ]
      },
      "android": {
        "gradleCommand": ":app:bundleProdRelease"
      },
      "ios": {
        "scheme": "ProdTrainLCD",
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "development": {
      "android": {
        "track": "internal",
        "applicationId": "me.tinykitten.trainlcd.dev"
      },
      "ios": {
        "ascAppId": "6450133462",
        "bundleIdentifier": "me.tinykitten.trainlcd.dev"
      }
    },
    "production": {
      "android": {
        "track": "internal",
        "applicationId": "me.tinykitten.trainlcd"
      },
      "ios": {
        "ascAppId": "6450133462",
        "bundleIdentifier": "me.tinykitten.trainlcd"
      }
    }
  }
}
